{
  "name": "jasmine-jquery-matchers",
  "description": "Add a set of custom matchers for HTML and CSS related checks, using jQuery.",
  "version": "1.3.0",
  "main": "dist/jasmine-jquery-matchers.js",
  "typings": "index.d.ts",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/unindented/jasmine-jquery-matchers.git"
  },
  "keywords": [
    "jasmine",
    "jquery",
    "matchers"
  ],
  "scripts": {
    "clean": "rimraf coverage dist",
    "test:lint": "eslint .",
    "test:tsc": "node test/typescript/index.js",
    "test:karma": "karma start --single-run",
    "test": "npm run test:lint && npm run test:tsc && npm run test:karma",
    "watch": "karma start",
    "build": "webpack --colors --progress --profile",
    "optim": "webpack --colors --progress --profile -p",
    "prepublish": "npm run clean && npm run build"
  },
  "peerDependencies": {
    "jquery": "*"
  },
  "devDependencies": {
    "@types/jasmine": "^2.5.37",
    "@types/jquery": "^2.0.33",
    "babel-core": "^6.18.0",
    "babel-loader": "^6.2.7",
    "babel-preset-es2015": "^6.18.0",
    "coveralls": "^2.11.14",
    "eslint": "^3.9.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.3.0",
    "eslint-plugin-standard": "^2.0.1",
    "istanbul-instrumenter-loader": "^1.0.0",
    "jasmine-core": "^2.5.2",
    "jquery": "^3.1.1",
    "karma": "^1.3.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.1.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "phantomjs-prebuilt": "^2.1.13",
    "rimraf": "^2.5.4",
    "typescript": "^2.0.6",
    "typescript-definition-tester": "^0.0.5",
    "webpack": "^1.13"
  }
}
